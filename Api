import axios from 'axios';

const apiKey = process.env.API_KEY;
const apiUrl = `https://api.example.com/data?apiKey=${apiKey}`;

export const fetchData = async () => {
  try {
    const response = await axios.get(apiUrl);
    return response.data;
  } catch (error) {
    console.error(error);
  }
};
